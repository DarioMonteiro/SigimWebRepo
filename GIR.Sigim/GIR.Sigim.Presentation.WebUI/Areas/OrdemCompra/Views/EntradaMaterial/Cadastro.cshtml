                                                    </section>
                                                    <section class="col col-3">
                                                        @Html.LabelFor(m => m.DataVencimento, new { @class = "label" })
                                                        <label class="input">
                                                            <i class="icon-append fa fa-calendar"></i>
                                                            @Html.TextBoxFor(m => m.DataVencimento, "{0:dd/MM/yyyy}", new { @readonly = "", @class = "readonly" })
                                                        </label>
                                                        @Html.ValidationMessageFor(m => m.DataVencimento, "Informe uma data válida.", new { @class = "note note-error" })
                                                    </section>
                                                    <section class="col col-3">
                                                        @Html.LabelFor(m => m.QuantidadeDiasAposEmissao, new { @class = "label" })
                                                        <label class="input">
                                                            @Html.TextBoxFor(m => m.QuantidadeDiasAposEmissao, new { @class = "text-right numeric" })
                                                        </label>
                                                        @Html.ValidationMessageFor(m => m.QuantidadeDiasAposEmissao, "", new { @class = "note note-error" })
                                                    </section>
                                                    <section class="col col-3">
                                                        @Html.LabelFor(m => m.DataVencimentoLiberado, new { @class = "label" })
                                                        <label class="input">
                                                            <i class="icon-append fa fa-calendar"></i>
                                                            @Html.TextBoxFor(m => m.DataVencimentoLiberado, "{0:dd/MM/yyyy}", new { @class = "" })
                                                        </label>
                                                        @Html.ValidationMessageFor(m => m.DataVencimentoLiberado, "Informe uma data válida.", new { @class = "note note-error" })
                                                    </section>
                                                    <script type="text/javascript">
                                                        $(document).ready(function () {
                                                            $('#@Html.IdFor(m => m.DataVencimentoLiberado)').datepicker({
                                                                prevText: '<i class="fa fa-chevron-left"></i>',
                                                                nextText: '<i class="fa fa-chevron-right"></i>',
                                                                changeMonth: true,
                                                                changeYear: true,
                                                                onClose: function (selectedDate) {
                                                                    $(this).valid();
                                                                    ajustarQuantidadeDiasAposEmissao();
                                                                }
                                                            });

                                                            $(document).on("blur", "#@Html.IdFor(m => m.QuantidadeDiasAposEmissao)", function () {
                                                                ajustarDataVencimentoLiberado();
                                                            });
                                                        });

                                                        function ajustarQuantidadeDiasAposEmissao() {
                                                            if ($("#@Html.IdFor(m => m.EntradaMaterial.DataEmissaoNota)").val() != "" && $("#@Html.IdFor(m => m.DataVencimentoLiberado)").val() != "") {
                                                                if (isValidDate("dd/mm/yy", $("#@Html.IdFor(m => m.EntradaMaterial.DataEmissaoNota)").val()) && isValidDate("dd/mm/yy", $("#@Html.IdFor(m => m.DataVencimentoLiberado)").val())) {
                                                                    var start = $("#@Html.IdFor(m => m.EntradaMaterial.DataEmissaoNota)").datepicker('getDate');
                                                                    var end = $("#@Html.IdFor(m => m.DataVencimentoLiberado)").datepicker('getDate');
                                                                    var days = dateDiffInDays(start, end);
                                                                    $("#@Html.IdFor(m => m.QuantidadeDiasAposEmissao)").val(roundDecimal(floatToString(days), 0));
                                                                }
                                                            }
                                                        }

                                                        function ajustarDataVencimentoLiberado() {
                                                            var format = "dd/mm/yy";
                                                            var dataEmissaoNota = $("#@Html.IdFor(m => m.EntradaMaterial.DataEmissaoNota)");
                                                            var dataVencimentoLiberado = $("#@Html.IdFor(m => m.DataVencimentoLiberado)");
                                                            var quantidadeDiasAposEmissao = $("#@Html.IdFor(m => m.QuantidadeDiasAposEmissao)");

                                                            if (quantidadeDiasAposEmissao.val() == '') quantidadeDiasAposEmissao.val(0);

                                                            if (dataEmissaoNota.val() != "" && quantidadeDiasAposEmissao.val() != "") {
                                                                if (isValidDate(format, $("#@Html.IdFor(m => m.EntradaMaterial.DataEmissaoNota)").val()) && $.isNumeric($("#@Html.IdFor(m => m.QuantidadeDiasAposEmissao)").val())) {
                                                                    var data = jQuery.datepicker.parseDate(format, dataEmissaoNota.val());
                                                                    data.setDate(data.getDate(format) + parseInt(quantidadeDiasAposEmissao.val()));
                                                                    dataVencimentoLiberado.datepicker('setDate', data);
                                                                }
                                                            }
                                                        }
                                                    </script>
                                                </div>
                                                <div class="row">
                                                    <section class="col col-3">
                                                        @Html.LabelFor(m => m.ValorParcela, new { @class = "label" })
                                                        <label class="input">
                                                            @Html.TextBoxFor(m => m.ValorParcela, "{0:F2}", new { @readonly = "", @class = "readonly" })
                                                        </label>
                                                        @Html.ValidationMessageFor(m => m.ValorParcela, "", new { @class = "note note-error" })
                                                    </section>
                                                    <section class="col col-3">
                                                        @Html.LabelFor(m => m.ValorParcelaLiberado, new { @class = "label" })
                                                        <label class="input">
                                                            @Html.TextBoxFor(m => m.ValorParcelaLiberado, "{0:F2}", new { @class = "text-right decimal-2-casas" })
                                                        </label>
