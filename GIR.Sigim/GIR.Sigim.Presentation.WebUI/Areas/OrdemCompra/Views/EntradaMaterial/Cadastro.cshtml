                                                        </tbody>
                                                    </table>
                                                </div>
                                                <div class="row">
                                                    <section class="col col-12">
                                                        <button id="btnIncluirFormaPagamentoEntradaMaterial" type="button" class="btn btn-primary right">
                                                            Incluir na Entrada de Material
                                                        </button>
                                                    </section>
                                                </div>
                                                <div class="row">
                                                    <section class="col col-3">
                                                        @Html.LabelFor(m => m.ValorTotalEMSelecaoFormaPagamento, new { @class = "label" })
                                                        <label class="input">
                                                            @Html.TextBoxFor(m => m.ValorTotalEMSelecaoFormaPagamento, "{0:F2}", new { @readonly = "", @class = "text-right decimal-2-casas readonly" })
                                                        </label>
                                                    </section>
                                                    <section class="col col-3">
                                                        @Html.LabelFor(m => m.ValorTotalDistribuidoSelecaoFormaPagamento, new { @class = "label" })
                                                        <label class="input">
                                                            @Html.TextBoxFor(m => m.ValorTotalDistribuidoSelecaoFormaPagamento, "{0:F2}", new { @readonly = "", @class = "text-right decimal-2-casas readonly" })
                                                        </label>
                                                    </section>
                                                    <section class="col col-3">
                                                        @Html.LabelFor(m => m.ValorTotalPendenteSelecaoFormaPagamento, new { @class = "label" })
                                                        <label class="input">
                                                            @Html.TextBoxFor(m => m.ValorTotalPendenteSelecaoFormaPagamento, "{0:F2}", new { @readonly = "", @class = "text-right decimal-2-casas readonly" })
                                                        </label>
                                                    </section>
                                                    <section class="col col-3">
                                                        @Html.LabelFor(m => m.ValorTotalLiberadoSelecaoFormaPagamento, new { @class = "label" })
                                                        <label class="input">
                                                            @Html.TextBoxFor(m => m.ValorTotalLiberadoSelecaoFormaPagamento, "{0:F2}", new { @readonly = "", @class = "text-right decimal-2-casas readonly" })
                                                        </label>
                                                    </section>
                                                </div>
                                                <div class="table-responsive" style="min-height: 85px; border: 1px solid #ddd; margin-bottom: 13px; overflow-x: auto;">
                                                    <table id="tableOrdemCompraValor" class="table table-bordered table-striped table-condensed table-hover dataTable">
                                                        <thead>
                                                            <tr role="row">
                                                                <th>OC</th>
                                                                <th class="text-right">Valor da OC</th>
                                                                <th class="text-right">Valor distribuído</th>
                                                                <th class="text-right">Valor pendente</th>
                                                            </tr>
                                                        </thead>
                                                        <tbody>
                                                        </tbody>
                                                    </table>
                                                </div>
                                            </fieldset>
                                            <script type="text/javascript">
                                                var jsonFormasPagamentoPendentes = [];
                                                var jsonResumoValoresOC = [];
                                                var ordemCompraIds = [];

                                                $(document).ready(function () {
                                                    $('#btnFormasPagamentoOrdemCompra').on('click', function () {
                                                        if (!$('#collapseFormasPagamentoOrdemCompra').is(":visible")) {
                                                            jsonFormasPagamentoPendentes = [];
                                                            jsonResumoValoresOC = [];
                                                            ordemCompraIds = obterIdsOrdensCompraUtilizadasNaEntradaMaterial();

                                                            if (!houveAlteracaoNoFormulario()) {
                                                                $.ajax({
                                                                    type: 'POST',
                                                                    url: '@Url.Action("ListarFormasPagamentoOrdemCompraPendentes", "EntradaMaterial")',
                                                                    cache: false,
                                                                    async: false,
                                                                    contentType: 'application/json; charset=utf-8',
                                                                    data: JSON.stringify({
                                                                        ordemCompraIds: ordemCompraIds
                                                                    })
                                                                })
                                                                .success(function (result) {
                                                                    jsonFormasPagamentoPendentes = JSON.parse(result);
                                                                });
                                                            }
                                                            loadPainelFormaPagamento();
                                                        }
                                                    });

                                                    $('#btnAddFormaPagamento').on('click', function () {
                                                        if ($('#formSelecaoFormaPagamento input').valid()) {
                                                            addFormaPagamento();
                                                        }
                                                    });

                                                    $('#btnIncluirFormaPagamentoEntradaMaterial').on('click', function () {
                                                        if ($('#ValorTotalEMSelecaoFormaPagamento').val() != $('#ValorTotalDistribuidoSelecaoFormaPagamento').val()) {
                                                            smartAlert("Erro", "Valor distribuído diferente do valor da Entrada de Material.", "error");
                                                        }
                                                        else {
                                                            var arr = jQuery.grep(jsonFormasPagamentoPendentes, function (formaPagamento, i) {
                                                                return (formaPagamento.DataVencimentoLiberado != null);
                                                            });

                                                            if (arr.length == 0) {
                                                                smartAlert("Erro", "Adicione pelo menos 1 forma de pagamento.", "error");
                                                            }
                                                            else {
                                                                var arrFormasPagamentoAdicionadas = [];
                                                                $.each(arr, function (i, formaPagamento) {
                                                                    var jsonFormaPagamento = {};
                                                                    jsonFormaPagamento.OrdemCompraFormaPagamentoId = formaPagamento.Id;
                                                                    jsonFormaPagamento.Data = formaPagamento.DataVencimentoLiberado;
                                                                    jsonFormaPagamento.Valor = parseFloat(formaPagamento.ValorParcelaLiberado.toFixed(2));
                                                                    arrFormasPagamentoAdicionadas.push(jsonFormaPagamento);
                                                                });

                                                                $.ajax({
                                                                    type: 'POST',
                                                                    url: '@Url.Action("AdicionarFormasPagamento", "EntradaMaterial")',
                                                                    cache: false,
                                                                    async: false,
                                                                    contentType: 'application/json; charset=utf-8',
                                                                    data: JSON.stringify({
                                                                        entradaMaterialId: '@Model.EntradaMaterial.Id',
                                                                        formasPagamento: JSON.stringify(arrFormasPagamentoAdicionadas)
                                                                    })
                                                                })
                                                                .success(function (result) {
                                                                    var houveErro = false;
                                                                    $(result.Messages).each(function () {
                                                                        if (this.Type.toString() == '@((int)GIR.Sigim.Infrastructure.Crosscutting.Notification.TypeMessage.Error)') {
                                                                            houveErro = true;
                                                                        }
                                                                    });

                                                                    if (!houveErro) {
                                                                        $('#@Html.IdFor(m => m.JsonFormasPagamento)').val(result.FormasPagamento);
                                                                        fillTableFormasPagamento();
                                                                        atualizarTotaisFormaPagamento();
                                                                        $('#btnFormasPagamento').click();
                                                                        setTimeout(function () {
                                                                            $('html, body').animate({
                                                                                scrollTop: $("#headerPanelFormasPagamento").offset().top
                                                                            }, 500);
                                                                        }, 500);
                                                                    }

                                                                    showMessages(result.Messages);
                                                                });
                                                            }
                                                        }
                                                    });
                                                });

                                                function loadPainelFormaPagamento() {
                                                    fillTableOrdemCompraFormaPagamento();
                                                    atualizarTotaisSelecaoFormaPagamento();
                                                    jsonResumoValoresOC = obterResumoValoresOC(ordemCompraIds);
                                                    fillTableOrdemCompraValor(jsonResumoValoresOC);
                                                }

                                                function addFormaPagamento() {
                                                    var jsonObj = $("#formSelecaoFormaPagamento").toObject({ mode: 'combine', skipEmpty: false, nodeCallback: processDataFormaPagamento })

                                                    $.each(jsonFormasPagamentoPendentes, function (i, formaPagamento) {
                                                        if ($('#@Html.IdFor(m => m.FormaPagamentoId)').val() == formaPagamento.Id) {
                                                            formaPagamento.DataVencimentoLiberado = jsonObj.DataVencimentoLiberado;
                                                            formaPagamento.ValorParcelaLiberado = jsonObj.ValorParcelaLiberado;
                                                            return false;
                                                        }
                                                    });

                                                    clearFormFormaPagamento();
                                                    loadPainelFormaPagamento();
                                                }

                                                function processDataFormaPagamento(node) {
                                                    var fieldId = node.getAttribute ? node.getAttribute('id') : '';
                                                    var fieldName = node.getAttribute ? node.getAttribute('name') : '';

                                                    if (fieldName != '' && (fieldId == "@Html.IdFor(m => m.DataVencimentoLiberado)")) {
                                                        var date = $("#" + fieldId).datepicker("getDate");
                                                        var jsonDate = "";
                                                        if (date != null) {
                                                            date.setHours(0, -date.getTimezoneOffset(), 0, 0); //removing the timezone offset.
                                                            jsonDate = date.toJSON();
                                                        }
                                                        return { name: fieldName, value: jsonDate };
                                                    }

                                                    if (fieldName != '' && (fieldId == "@Html.IdFor(m => m.ValorParcelaLiberado)")) {
                                                        var value = $("#" + fieldId).val();
                                                        return { name: fieldName, value: parseFloat(value.toString().replace(".", "").replace(",", ".")) };
                                                    }

                                                    return false;
                                                }

                                                function fillTableOrdemCompraFormaPagamento() {
                                                    $("#tableOrdemCompraFormaPagamento tbody").empty();
                                                    for (var i = 0; i < jsonFormasPagamentoPendentes.length; i++) {
                                                        var row = $('<tr />')
                                                        $("#tableOrdemCompraFormaPagamento tbody").append(row);
                                                        row.append($('<td class="text-right" onclick="loadFormaPagamento(' + jsonFormasPagamentoPendentes[i].Id + ');">' + jsonFormasPagamentoPendentes[i].OrdemCompraId + '</td>'));
                                                        row.append($('<td class="text-center">' + new Date(jsonFormasPagamentoPendentes[i].Data).toFormatDDMMYYYY() + '</td>'));
                                                        row.append($('<td class="text-right decimal-2-casas">' + roundDecimal(floatToString(jsonFormasPagamentoPendentes[i].Valor), 2) + '</td>'));
                                                        if (jsonFormasPagamentoPendentes[i].TipoCompromisso != null)
                                                            row.append($('<td class="text-center">' + jsonFormasPagamentoPendentes[i].TipoCompromisso.Descricao + '</td>'));
                                                        else
                                                            row.append($('<td>&nbsp;</td>'));

                                                        if (jsonFormasPagamentoPendentes[i].TituloPagar != null)
                                                            row.append($('<td class="text-center">' + jsonFormasPagamentoPendentes[i].TituloPagar.Id+ '</td>'));
                                                        else
                                                            row.append($('<td>&nbsp;</td>'));

                                                        if (jsonFormasPagamentoPendentes[i].DataVencimentoLiberado != null)
                                                            row.append($('<td class="text-center">' + new Date(jsonFormasPagamentoPendentes[i].DataVencimentoLiberado).toFormatDDMMYYYY() + '</td>'));
                                                        else
                                                            row.append($('<td>&nbsp;</td>'));
                                                    
                                                        if (jsonFormasPagamentoPendentes[i].ValorParcelaLiberado != null)
                                                            row.append($('<td class="text-right decimal-2-casas">' + roundDecimal(floatToString(jsonFormasPagamentoPendentes[i].ValorParcelaLiberado), 2) + '</td>'));
                                                        else
                                                            row.append($('<td>&nbsp;</td>'));
                                                    }

                                                    initializeDecimalBehaviour();
                                                }

                                                function loadFormaPagamento(id) {
                                                    clearFormFormaPagamento();
                                                    var arr = jQuery.grep(jsonFormasPagamentoPendentes, function (formaPagamento, i) {
                                                        return (formaPagamento.Id == id);
                                                    });

                                                    if (arr.length > 0) {
                                                        var formaPagamento = arr[0];
                                                        $("#@Html.IdFor(m => m.FormaPagamentoId)").val(formaPagamento.Id);
                                                        $("#@Html.IdFor(m => m.OrdemCompraId)").val(formaPagamento.OrdemCompraId);
                                                        $("#@Html.IdFor(m => m.DataVencimento)").val(new Date(formaPagamento.Data).toFormatDDMMYYYY());

                                                        var dataVencimentoLiberado = formaPagamento.DataVencimentoLiberado == null ? formaPagamento.Data : formaPagamento.DataVencimentoLiberado;
                                                        $("#@Html.IdFor(m => m.DataVencimentoLiberado)").val(new Date(dataVencimentoLiberado).toFormatDDMMYYYY());
                                                        $("#@Html.IdFor(m => m.ValorParcela)").val(roundDecimal(floatToString(formaPagamento.Valor), 2));

                                                        var valorParcelaLiberado = formaPagamento.ValorParcelaLiberado == null ? formaPagamento.Valor : formaPagamento.ValorParcelaLiberado;
                                                        $("#@Html.IdFor(m => m.ValorParcelaLiberado)").val(roundDecimal(floatToString(valorParcelaLiberado), 2));
                                                        ajustarQuantidadeDiasAposEmissao();
                                                    }

                                                    initializeDecimalBehaviour();
                                                }

                                                function clearFormFormaPagamento() {
                                                    $("#@Html.IdFor(m => m.FormaPagamentoId)").val('');
                                                    $("#@Html.IdFor(m => m.OrdemCompraId)").val('');
                                                    $("#@Html.IdFor(m => m.DataVencimento)").val('');
                                                    $("#@Html.IdFor(m => m.QuantidadeDiasAposEmissao)").val('');
                                                    $("#@Html.IdFor(m => m.DataVencimentoLiberado)").val('');
                                                    $("#@Html.IdFor(m => m.ValorParcela)").val(roundDecimal('0', 2));
                                                    $("#@Html.IdFor(m => m.ValorParcelaLiberado)").val(roundDecimal('0', 2));
                                                }

                                                function atualizarTotaisSelecaoFormaPagamento() {
                                                    var valorTotalEM = parseFloat($("#@Html.IdFor(m => m.ValorTotalEMSelecaoFormaPagamento)").val().replace(".", "").replace(",", "."));
                                                    var valorTotalDistribuido = parseFloat($("#@Html.IdFor(m => m.ValorTotalDistribuido)").val().replace(".", "").replace(",", "."));
                                                    var valorTotalLiberado = parseFloat($("#@Html.IdFor(m => m.ValorTotalLiberado)").val().replace(".", "").replace(",", "."));

                                                    $.each(jsonFormasPagamentoPendentes, function (i, formaPagamento) {
                                                        if (formaPagamento.ValorParcelaLiberado != null) {
                                                            valorTotalDistribuido += formaPagamento.ValorParcelaLiberado;
                                                        }
                                                    });

                                                    $("#@Html.IdFor(m => m.ValorTotalDistribuidoSelecaoFormaPagamento)").val(roundDecimal(floatToString(valorTotalDistribuido), 2));
                                                    $("#@Html.IdFor(m => m.ValorTotalLiberadoSelecaoFormaPagamento)").val(roundDecimal(floatToString(valorTotalLiberado), 2));
                                                    $("#@Html.IdFor(m => m.ValorTotalPendenteSelecaoFormaPagamento)").val(roundDecimal(floatToString(valorTotalEM - valorTotalDistribuido), 2));

                                                    initializeDecimalBehaviour();
                                                }

                                                function obterIdsOrdensCompraUtilizadasNaEntradaMaterial() {
                                                    var jsonItens = JSON.parse($("#@Html.IdFor(m => m.JsonItens)").val());
                                                    var ids = [];

                                                    $.each(jsonItens, function (index, item) {
                                                        if ($.inArray(item.OrdemCompraItem.OrdemCompraId, ids) === -1) {
                                                            ids.push(item.OrdemCompraItem.OrdemCompraId);
                                                        }
                                                    });

                                                    return ids;
                                                }

                                                function obterResumoValoresOC(ordemCompraIds) {
                                                    var jsonItens = JSON.parse($("#@Html.IdFor(m => m.JsonItens)").val());
                                                    var resumoOC = [];

                                                    $.each(ordemCompraIds, function (index, ordenCompraId) {
                                                        var valorTotalItens = 0;
                                                        $.each(jsonItens, function (i, item) {
                                                            if (item.OrdemCompraItem.OrdemCompraId == ordenCompraId) {
                                                                var valorIPI = item.ValorTotal * item.PercentualIPI / 100;
                                                                valorTotalItens += item.ValorTotal + valorIPI - (item.ValorTotal * item.PercentualDesconto / 100);
                                                            }
                                                        });

                                                        var jsonFormasPagamento = JSON.parse($("#@Html.IdFor(m => m.JsonFormasPagamento)").val());
                                                        var valorTotalDistribuido = 0;
                                                        $.each(jsonFormasPagamento, function (i, formaPagamento) {
                                                            if (formaPagamento.OrdemCompraFormaPagamento.OrdemCompraId == ordenCompraId) {
                                                                valorTotalDistribuido += formaPagamento.Valor;
                                                            }
                                                        });

                                                        var valorTotalDistribuidoProvisorio = valorTotalDistribuido
                                                        $.each(jsonFormasPagamentoPendentes, function (i, formaPagamento) {
                                                            if ((formaPagamento.OrdemCompraId == ordenCompraId) && (formaPagamento.ValorParcelaLiberado != null)) {
                                                                valorTotalDistribuidoProvisorio += formaPagamento.ValorParcelaLiberado;
                                                            }
                                                        });

                                                        var jsonOC = {};
                                                        jsonOC.Id = ordenCompraId;
                                                        jsonOC.ValorTotalItens = parseFloat(valorTotalItens.toFixed(2));
                                                        jsonOC.ValorTotalDistribuido = parseFloat(valorTotalDistribuido.toFixed(2));
                                                        jsonOC.ValorTotalDistribuidoProvisorio = parseFloat(valorTotalDistribuidoProvisorio.toFixed(2));
                                                        jsonOC.ValorPendente = parseFloat(valorTotalItens - valorTotalDistribuido).toFixed(2);
                                                        jsonOC.ValorPendenteProvisorio = parseFloat(valorTotalItens - valorTotalDistribuidoProvisorio).toFixed(2);
                                                        resumoOC.push(jsonOC);
                                                    });

                                                    return resumoOC;
                                                }

                                                function fillTableOrdemCompraValor(resumoValoresOC) {
                                                    $("#tableOrdemCompraValor tbody").empty();
                                                    for (var i = 0; i < resumoValoresOC.length; i++) {
                                                        var row = $('<tr />')
                                                        $("#tableOrdemCompraValor tbody").append(row);
                                                        row.append($('<td>' + resumoValoresOC[i].Id + '</td>'));
                                                        row.append($('<td class="text-right decimal-2-casas">' + roundDecimal(floatToString(resumoValoresOC[i].ValorTotalItens), 2) + '</td>'));
                                                        var valorTotalDistribuido = resumoValoresOC[i].ValorTotalDistribuidoProvisorio == null ? resumoValoresOC[i].ValorTotalDistribuido : resumoValoresOC[i].ValorTotalDistribuidoProvisorio;
                                                        row.append($('<td class="text-right decimal-2-casas">' + roundDecimal(floatToString(valorTotalDistribuido), 2) + '</td>'));
                                                        var valorPendente = resumoValoresOC[i].ValorPendenteProvisorio == null ? resumoValoresOC[i].ValorPendente : resumoValoresOC[i].ValorPendenteProvisorio;
                                                        row.append($('<td class="text-right decimal-2-casas">' + roundDecimal(floatToString(valorPendente), 2) + '</td>'));
                                                    }

                                                    initializeDecimalBehaviour();
                                                }
                                            </script>
                                        </div>
                                    </div>
                                </div>
                            }
