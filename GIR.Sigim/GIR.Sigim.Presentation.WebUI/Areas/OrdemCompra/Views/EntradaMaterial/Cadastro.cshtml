                                                        <i class="icon-append fa fa-calendar"></i>
                                                        @Html.TextBoxFor(m => m.EntradaMaterial.DataFrete, "{0:dd/MM/yyyy}", new { @class = "exigeGravacao" })
                                                    </label>
                                                    @Html.ValidationMessageFor(m => m.EntradaMaterial.DataFrete, "Informe uma data válida.", new { @class = "note note-error" })
                                                </section>
                                                <script type="text/javascript">
                                                    $(document).ready(function () {
                                                        $('#@Html.IdFor(m => m.EntradaMaterial.DataFrete)').datepicker({
                                                            prevText: '<i class="fa fa-chevron-left"></i>',
                                                            nextText: '<i class="fa fa-chevron-right"></i>',
                                                            changeMonth: true,
                                                            changeYear: true,
                                                            onClose: function (selectedDate) {
                                                                $(this).valid();
                                                            }
                                                        });
                                                    });
                                                </script>
                                                <section class="col col-4">
                                                    @Html.LabelFor(m => m.EntradaMaterial.ValorFrete, new { @class = "label" })
                                                    <label class="input">
                                                        @Html.TextBoxFor(m => m.EntradaMaterial.ValorFrete, "{0:F2}", new { @readonly = "", @class = "text-right decimal-2-casas readonly" })
                                                        <i></i>
                                                    </label>
                                                    @Html.ValidationMessageFor(m => m.EntradaMaterial.ValorFrete, "", new { @class = "note note-error" })
                                                </section>
                                            </div>
                                            <div class="row">
                                                <section class="col col-3">
                                                    @Html.LabelFor(m => m.EntradaMaterial.OrdemCompraFreteId, new { @class = "label" })
