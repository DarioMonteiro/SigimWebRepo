                                                        <thead>
                                                            <tr role="row">
                                                                <th class="text-right">OC</th>
                                                                <th class="text-center">Data vencimento</th>
                                                                <th class="text-right">Valor parcela</th>
                                                                <th>Compromisso</th>
                                                                <th class="text-right">Título pagar</th>
                                                                <th class="text-center">Data vencimento lib.</th>
                                                                <th class="text-right">Valor liberado</th>
                                                            </tr>
