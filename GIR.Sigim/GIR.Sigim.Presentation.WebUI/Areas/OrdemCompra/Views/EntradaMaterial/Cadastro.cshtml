                                                    @if (Model.PodeEditarImposto)
                                                    {
                                                        @:row.append($('<td class="text-nowrap" onclick="loadImposto(' + jsonImpostos[i].ImpostoFinanceiro.Id + ');">' + jsonImpostos[i].ImpostoFinanceiro.Descricao + '</td>'));
	                                                }
                                                    else
                                                    {
                                                        @:row.append($('<td class="text-nowrap">' + jsonImpostos[i].ImpostoFinanceiro.Descricao + '</td>'));
                                                    }
                                                    row.append($('<td class="text-right decimal-5-casas">' + roundDecimal(floatToString(jsonImpostos[i].BaseCalculo), 5) + '</td>'));
                                                    row.append($('<td class="text-right decimal-5-casas">' + roundDecimal(floatToString(jsonImpostos[i].ImpostoFinanceiro.Aliquota), 5) + '</td>'));
                                                    row.append($('<td class="text-right decimal-5-casas">' + roundDecimal(floatToString(jsonImpostos[i].Valor), 5) + '</td>'));
                                                    if (jsonImpostos[i].DataVencimento != null)
                                                        row.append($('<td class="text-center">' + new Date(jsonImpostos[i].DataVencimento).toFormatDDMMYYYY() + '</td>'));
                                                    else
                                                        row.append($('<td>&nbsp;</td>'));
                                                }

                                                initializeDecimalBehaviour();
                                            }

                                            function loadImposto(impostoFinanceiroId) {
                                                clearFormImposto();
                                                var jsonImpostos = JSON.parse($("#@Html.IdFor(m => m.JsonImpostos)").val());
                                                var index = -1;
                                                var arr = jQuery.map(jsonImpostos, function (item, i) {
                                                    if (item.ImpostoFinanceiro.Id == impostoFinanceiroId) {
                                                        index = i;
                                                        return item;
                                                    }
                                                });

                                                if (arr.length > 0) {
                                                    var imposto = arr[0];

                                                    $("#indexImposto").val(index);
                                                    $("#impostoId").val(imposto.Id);
                                                    if (imposto.ImpostoFinanceiro)
                                                        $("#@Html.IdFor(m => m.ImpostoFinanceiroId)").val(imposto.ImpostoFinanceiro.Id);

                                                    $("#@Html.IdFor(m => m.BaseCalculoImposto)").val(roundDecimal(floatToString(imposto.BaseCalculo), 5));

                                                    if (imposto.DataVencimento != null)
                                                        $("#@Html.IdFor(m => m.DataVencimentoImposto)").val(new Date(imposto.DataVencimento).toFormatDDMMYYYY());
                                                }

                                                initializeDecimalBehaviour();
                                            }

                                            function clearFormImposto() {
                                                $("#indexImposto").val('');
                                                $("#impostoId").val('');
                                                $("#@Html.IdFor(m => m.ImpostoFinanceiroId)").val('');
                                                var valorTotalITem = parseFloat($("#@Html.IdFor(m => m.ValorTotalItem)").val().replace(".", "").replace(",", "."));
                                                $("#@Html.IdFor(m => m.BaseCalculoImposto)").val(roundDecimal(floatToString(valorTotalITem), 2));
                                                $("#@Html.IdFor(m => m.DataVencimentoImposto)").val('');
                                            }

                                            function ehImpostoRepetido() {
                                                var jsonImposto = JSON.parse($("#@Html.IdFor(m => m.JsonImpostos)").val());
                                                var index = parseInt($("#indexImposto").val());
                                                var result = false;
                                                $.each(jsonImposto, function (i, obj) {
                                                    if ((index != i) && (obj.ImpostoFinanceiro.Id == $('#@Html.IdFor(m => m.ImpostoFinanceiroId)').val())) {
                                                        result = true;
                                                        return false;
                                                    }
                                                });
                                                return result;
                                            }

                                            function addImposto() {
                                                var imposto = $("#formImposto").toObject({ mode: 'combine', skipEmpty: false, nodeCallback: processDataImposto })

                                                $.ajax({
                                                    type: 'POST',
                                                    url: '@Url.Action("CarregarItem", "ImpostoFinanceiro", new { @area = "Financeiro" })',
                                                    cache: false,
                                                    async: false,
                                                    contentType: 'application/json; charset=utf-8',
                                                    data: JSON.stringify({
                                                        id: imposto.ImpostoFinanceiroId
                                                    })
                                                })
                                                .success(function (result) {
                                                    imposto["ImpostoFinanceiro"] = result;
                                                    var valor = (imposto["BaseCalculo"] * result.Aliquota / 100).toFixed(2);
                                                    imposto["Valor"] = parseFloat(valor);
                                                });

                                                var jsonImpostos = JSON.parse($("#@Html.IdFor(m => m.JsonImpostos)").val());
                                                var index = parseInt($("#indexImposto").val());

                                                if (isNaN(index))
                                                    jsonImpostos.push(imposto);
                                                else
                                                    jsonImpostos.splice(index, 1, imposto);

                                                $("#@Html.IdFor(m => m.JsonImpostos)").val(JSON.stringify(jsonImpostos));
                                                clearFormImposto();
                                                fillTableImpostos();
                                                atualizarTotais();
                                                ehFormularioAlterado = true;
                                            }

                                            function processDataImposto(node) {
                                                var fieldId = node.getAttribute ? node.getAttribute('id') : '';
                                                var fieldName = node.getAttribute ? node.getAttribute('name') : '';

                                                if (fieldName != '' && (fieldId == "impostoId")) {
                                                    return { name: "Id", value: $("#impostoId").val() };
                                                }

                                                if (fieldName != '' && (fieldId == "@Html.IdFor(m => m.BaseCalculoImposto)")) {
                                                    var value = $("#" + fieldId).val();
                                                    return { name: "BaseCalculo", value: parseFloat(value.toString().replace(".", "").replace(",", ".")) };
                                                }

                                                if (fieldName != '' && (fieldId == "@Html.IdFor(m => m.DataVencimentoImposto)")) {
                                                    var date = $("#" + fieldId).datepicker("getDate");
                                                    var jsonDate = "";
                                                    if (date != null) {
                                                        date.setHours(0, -date.getTimezoneOffset(), 0, 0); //removing the timezone offset.
                                                        jsonDate = date.toJSON();
                                                    }
                                                    return { name: "DataVencimento", value: jsonDate };
                                                }

                                                return false;
                                            }
                                        </script>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <footer>
                            @Html.ActionLink("Voltar", "Index", null, null, new { @class = "btn btn-primary" })
                            @{
                                string disableBtnCancelar = string.Empty;
                                if (!Model.PodeCancelarEntrada)
                                {
                                    disableBtnCancelar = "disabled";
                                }
                            }
                            <a id="btnCancelarEntrada" href="#" class="btn btn-danger @disableBtnCancelar">Cancelar</a>
                            @if (Model.PodeCancelarEntrada)
                            {
                                <div class="modal fade" id="MotivoCancelamento_ModalPanel" tabindex="-1" role="dialog">
                                    <div class="modal-dialog">
                                        <div class="modal-content">
                                            <div class="modal-header">
                                                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">
                                                    &times;
                                                </button>
                                                <h4 class="modal-title">
                                                    Motivo do Cancelamento
                                                </h4>
                                            </div>
                                            <div class="modal-body">
                                                @if (Model.ExisteEstoqueParaCentroCusto && !Model.ExisteMovimentoNoEstoque)
                                                {
                                                    <article class="col-sm-12">
                                                        <div class="alert alert-info alert-block fade in">
                                                            <h4 class="alert-heading">Informação</h4>
                                                            O cancelamento não irá abater a quantidade do insumo no estoque.
                                                            Porque, quando da entrada desse insumo, o sistema não o incluiu no estoque.<br />
                                                            Em caso de dúvida, favor anotar o número da Entrada de Material e reportar a GIR.
                                                        </div>
                                                    </article>
                                                }
                                                <label class="textarea">
                                                    @Html.TextAreaFor(m => m.EntradaMaterial.MotivoCancelamento, new { @rows = "3" })
                                                </label>
                                            </div>
                                            <div class="modal-footer">
								                <button type="button" id="btnConfirmaCancelamentoEntrada" class="btn btn-primary">
									                Ok
								                </button>
							                </div>
                                        </div>
                                    </div>
                                    <script type="text/javascript">
                                        $("#btnCancelarEntrada").on("click", function (e) {
                                            e.preventDefault();
                                            $.ajax({
                                                type: 'POST',
                                                url: '@Url.Action("HaPossibilidadeCancelamentoEntradaMaterial", "EntradaMaterial")',
                                                cache: false,
                                                //async: false,
                                                contentType: 'application/json; charset=utf-8',
                                                data: JSON.stringify({
                                                    entradaMaterialId: '@Html.DisplayFor(m => m.EntradaMaterial.Id)'
                                                })
                                            })
                                            .success(function (result) {
                                                if (result.HaPossibilidadeAcao)
                                                    $('#MotivoCancelamento_ModalPanel').modal('show');
                                                else {
                                                    var errorMessages = result.ErrorMessages;
                                                    if (errorMessages.length !== 0) {
                                                        for (var i = 0; i < errorMessages.length; i++)
                                                            smartAlert("Erro", errorMessages[i].Text, "error");
                                                    }
                                                }
                                            });
                                        });

                                        $("#btnConfirmaCancelamentoEntrada").on("click", function () {
                                            var motivo = $("#@Html.IdFor(m => m.EntradaMaterial.MotivoCancelamento)").val();
                                            if (motivo.length != 0) {
                                                $.ajax({
                                                    type: 'POST',
                                                    url: '@Url.Action("Cancelar", "EntradaMaterial")',
                                                    cache: false,
                                                    //async: false,
                                                    contentType: 'application/json; charset=utf-8',
                                                    data: JSON.stringify({
                                                        id: '@Html.DisplayFor(m => m.EntradaMaterial.Id)',
                                                        motivo: motivo
                                                    })
                                                })
                                                .success(function (result) {
                                                    $('#notificationMessages').html(result);
                                                });
                                            }
                                            else
                                                smartAlert("Erro", "@GIR.Sigim.Application.Resource.OrdemCompra.ErrorMessages.InformeMotivoCancelamentoEntradaMaterial", "error");
                                        });
                                    </script>
                                </div>
                            }

                            @{
                            var url = Url.Action("Imprimir", "EntradaMaterial") + "/" + @Html.DisplayFor(m => m.EntradaMaterial.Id);
                            }
                            @Html.EditorFor(m => m.PodeImprimir, "Imprimir", new { @url = url })
                            
                            @{
                            string disableBtnSalvar = string.Empty;
                            if (!Model.PodeSalvar)
                            {
                                disableBtnSalvar = "disabled";
                            }
                            }
                            <button id="btnSalvar" type="submit" class="btn btn-primary right @disableBtnSalvar">
                                Gravar
                            </button>
                        </footer>
                    }
                </div>
            </div>
        </div>
    </article>
</div>
<script type="text/javascript">
    var ehFormularioAlterado = false;

    function houveAlteracaoNoFormulario() {
        if (ehFormularioAlterado)
            smartAlert("Atenção", "A Entrada de Material foi alterada.<br />Salve os dados antes de executar esta ação.", "warning");

        return ehFormularioAlterado;
    }

    $(document).ready(function () {
        $('body').addClass("hidden-menu");

        atualizarTotais();

        $(document).on('input change', '.exigeGravacao', function () {
            ehFormularioAlterado = true;
        });

        $('#btnLiberarTitulos').on("click", function () {
            if (!houveAlteracaoNoFormulario()) {
                $.ajax({
                    type: 'POST',
                    url: '@Url.Action("HaPossibilidadeLiberacaoTitulos", "EntradaMaterial")',
                    cache: false,
                    //async: false,
                    contentType: 'application/json; charset=utf-8',
                    data: JSON.stringify({
                        entradaMaterialId: '@Html.DisplayFor(m => m.EntradaMaterial.Id)'
                    })
                })
                .success(function (result) {
                    if (result.HaPossibilidadeAcao)
                        liberarTitulos();
                    else {
                        var errorMessages = result.ErrorMessages;
                        if (errorMessages.length !== 0) {
                            for (var i = 0; i < errorMessages.length; i++)
                                smartAlert("Erro", errorMessages[i].Text, "error");
                        }
                    }
                });
            }
        });

        jQuery.validator.addMethod(
            "ehDataEntradaMaterialValida",
            function (value, element, params) {
                var isValid = false;
                $.ajax({
                    type: 'POST',
                    url: '@Url.Action("ValidaDataEntradaMaterial")',
                    cache: false,
                    async: false,
                    contentType: 'application/json; charset=utf-8',
                    data: JSON.stringify({
                        id: '@Model.EntradaMaterial.Id',
                        data: $("#@Html.IdFor(m => m.EntradaMaterial.Data)").val()
                    })
                })
                .success(function (result) {
                    showErrorMessage(getErrorMessageContainer('@Html.NameFor(m => m.EntradaMaterial.Data)'), result.errorMessage);
                    isValid = result.ehValido;
                });
                $.validator.messages.ehDataEntradaMaterialValida = getErrorMessageContainer('@Html.NameFor(m => m.EntradaMaterial.Data)').html();
                return isValid;
            }, ''
        );

        jQuery.validator.addMethod(
            "ehCentroCustoValido",
            function (value, element, params) {
                var isValid = validaCentroCusto();
                $.validator.messages.ehCentroCustoValido = $('#@Html.IdFor(m => m.EntradaMaterial.CentroCusto.Codigo)_ValidationMessage').html();
                return isValid;
            }, ''
        );

        jQuery.validator.addMethod(
            "ehNumeroNotaFiscalValido",
            function (value, element, params) {
                var isValid = true;
                if (($("#@Html.IdFor(m => m.EntradaMaterial.ClienteFornecedor.Id)").val() != '' || $("#@Html.IdFor(m => m.EntradaMaterial.FornecedorNota.Id)").val() != '')
                    && $("#@Html.IdFor(m => m.EntradaMaterial.NumeroNotaFiscal)").val() != ''
                    && $("#@Html.IdFor(m => m.EntradaMaterial.DataEmissaoNota)").val() != '') {
                    isValid = false;
                    $.ajax({
                        type: 'POST',
                        url: '@Url.Action("ValidaNumeroNotaFiscal")',
                        cache: false,
                        async: false,
                        contentType: 'application/json; charset=utf-8',
                        data: JSON.stringify({
                            '@Html.NameFor(m => m.EntradaMaterial.Id)': '@Model.EntradaMaterial.Id',
                            '@Html.NameFor(m => m.EntradaMaterial.ClienteFornecedor.Id)': $("#@Html.IdFor(m => m.EntradaMaterial.ClienteFornecedor.Id)").val(),
                            '@Html.NameFor(m => m.EntradaMaterial.FornecedorNota.Id)': $("#@Html.IdFor(m => m.EntradaMaterial.FornecedorNota.Id)").val(),
                            '@Html.NameFor(m => m.EntradaMaterial.NumeroNotaFiscal)': $("#@Html.IdFor(m => m.EntradaMaterial.NumeroNotaFiscal)").val(),
                            '@Html.NameFor(m => m.EntradaMaterial.DataEmissaoNota)': $("#@Html.IdFor(m => m.EntradaMaterial.DataEmissaoNota)").val()
                        })
                    })
                    .success(function (result) {
                        showErrorMessage(getErrorMessageContainer('@Html.NameFor(m => m.EntradaMaterial.NumeroNotaFiscal)'), result.errorMessage);
                        isValid = result.ehValido;
                    });
                    $.validator.messages.ehNumeroNotaFiscalValido = getErrorMessageContainer('@Html.NameFor(m => m.EntradaMaterial.NumeroNotaFiscal)').html();
                }

                return isValid;
            }, ''
        );

        jQuery.validator.addMethod(
            "ehDataEmissaoNotaValida",
            function (value, element, params) {
                var isValid = validaDataNota();
                $.validator.messages.ehDataEmissaoNotaValida = getErrorMessageContainer('@Html.NameFor(m => m.EntradaMaterial.DataEmissaoNota)').html();
                return isValid;
            }, ''
        );

        jQuery.validator.addMethod(
            "ehDataEntregaValida",
            function (value, element, params) {
                var isValid = validaDataNota();
                $.validator.messages.ehDataEntregaValida = getErrorMessageContainer('@Html.NameFor(m => m.EntradaMaterial.DataEntregaNota)').html();
                return isValid;
            }, ''
        );

        jQuery.validator.addMethod(
            "ehDataEntregaMenorOuIgualDataEM",
            function (value, element, params) {
                var dataEntradaMaterial = $('#@Html.IdFor(m => m.EntradaMaterial.Data)').datepicker("getDate");
                var dataEntrega = $('#@Html.IdFor(m => m.EntradaMaterial.DataEntregaNota)').datepicker("getDate");
                var isValid = dataEntrega <= dataEntradaMaterial;
                return isValid;
            }, ''
        );

        jQuery.validator.addMethod(
            "ehDataVencimentoNoLimitePagamento",
            function (value, element, params) {
                var diasParaPagamento = obterQuantidadeDiasMinimoParaPagamento();
                if (diasParaPagamento > 0) {
                    if ($("#@Html.IdFor(m => m.DataVencimentoLiberado)").val() != "" && isValidDate("dd/mm/yy", $("#@Html.IdFor(m => m.DataVencimentoLiberado)").val())) {
                        var start = $("#@Html.IdFor(m => m.EntradaMaterial.Data)").datepicker('getDate');
                        var end = $("#@Html.IdFor(m => m.DataVencimentoLiberado)").datepicker('getDate');
                        var days = dateDiffInDays(start, end);
                        return days >= diasParaPagamento;
                    }
                    return false;
                }
                return true;
            }, 'Data de vencimento fora do limite de pagamento.'
        );

        jQuery.validator.addMethod(
            "ehValorLiberadoMenorIgualPendente",
            function (value, element, params) {
                var ordemCompraId = parseInt($("#@Html.IdFor(m => m.OrdemCompraId)").val());
                var arr = jQuery.grep(jsonResumoValoresOC, function (ordemCompra, i) {
                    return (ordemCompra.Id == ordemCompraId);
                });

                if (arr.length > 0) {
                    var ordemCompra = arr[0];
                    return parseFloat(value.replace(".", "").replace(",", ".")) <= ordemCompra.ValorPendente;
                }

                return true;
            }, 'Valor inválido para ordem de compra.'
        );

        jQuery.validator.addMethod(
            "ehValorLiberadoMenorIgualParcela",
            function (value, element, params) {
                var valorParcela = parseFloat($('#@Html.IdFor(m => m.ValorParcela)').val().replace(".", "").replace(",", "."));
                var valorParcelaLiberado = parseFloat(value.replace(".", "").replace(",", "."));
                return valorParcelaLiberado <= valorParcela;
            }, 'Valor liberado maior que valor da parcela.'
        );

        jQuery.validator.setDefaults({
            ignore: '#formItem *, #formImposto *, #formSelecaoFormaPagamento *'
        });

        $('#formCadastro').validate({
            rules: {
                '@Html.NameFor(m => m.EntradaMaterial.Data)': {
                    required: true,
                    date: true,
                    ehDataEntradaMaterialValida: true
                },
                '@Html.NameFor(m => m.EntradaMaterial.CentroCusto.Codigo)': {
                    required: true,
                    ehCentroCustoValido: true
                },
                '@Html.NameFor(m => m.EntradaMaterial.ClienteFornecedor.Nome)': {
                    required: true
                },
                '@Html.NameFor(m => m.EntradaMaterial.TipoNotaFiscal.Id)': {
                    required: true
                },
                '@Html.NameFor(m => m.EntradaMaterial.NumeroNotaFiscal)': {
                    required: true,
                    ehNumeroNotaFiscalValido: true
                },
                '@Html.NameFor(m => m.EntradaMaterial.DataEmissaoNota)': {
                    required: true,
                    ehDataEmissaoNotaValida: true
                },
                '@Html.NameFor(m => m.EntradaMaterial.DataEntregaNota)': {
                    required: true,
                    ehDataEntregaValida: true,
                    ehDataEntregaMenorOuIgualDataEM: true
                },
                '@Html.NameFor(m => m.Quantidade)': 'decimalGreaterThanZero',
                '@Html.NameFor(m => m.Material.Id)': 'required',
                '@Html.NameFor(m => m.OrdemCompraId)': 'required',
                '@Html.NameFor(m => m.DataVencimentoLiberado)': {
                    required: true,
                    date: true,
                    ehDataVencimentoNoLimitePagamento: true
                },
                '@Html.NameFor(m => m.ValorParcelaLiberado)': {
                    decimalGreaterThanZero: true,
                    ehValorLiberadoMenorIgualPendente: true,
                    ehValorLiberadoMenorIgualParcela: true
                },
                '@Html.NameFor(m => m.ImpostoFinanceiroId)': 'required',
                '@Html.NameFor(m => m.BaseCalculoImposto)': 'required',
                '@Html.NameFor(m => m.DataVencimentoImposto)': 'date'
            },
            messages: {
                '@Html.NameFor(m => m.EntradaMaterial.Data)': {
                    required: '@GIR.Sigim.Application.Resource.Sigim.ErrorMessages.InformeDataValida',
                    date: '@GIR.Sigim.Application.Resource.Sigim.ErrorMessages.InformeDataValida'
                },
                '@Html.NameFor(m => m.EntradaMaterial.CentroCusto.Codigo)': {
                    required: '@string.Format(GIR.Sigim.Application.Resource.Sigim.ErrorMessages.CampoObrigatorio, "Centro de Custo")'
                },
                '@Html.NameFor(m => m.EntradaMaterial.ClienteFornecedor.Nome)': {
                    required: '@string.Format(GIR.Sigim.Application.Resource.Sigim.ErrorMessages.CampoObrigatorio, "Fornecedor")'
                },
                '@Html.NameFor(m => m.EntradaMaterial.TipoNotaFiscal.Id)': {
                    required: '@string.Format(GIR.Sigim.Application.Resource.Sigim.ErrorMessages.CampoObrigatorio, "Tipo")'
                },
                '@Html.NameFor(m => m.EntradaMaterial.NumeroNotaFiscal)': {
                    required: '@string.Format(GIR.Sigim.Application.Resource.Sigim.ErrorMessages.CampoObrigatorio, "Número")'
                },
                '@Html.NameFor(m => m.EntradaMaterial.DataEmissaoNota)': {
                    required: '@string.Format(GIR.Sigim.Application.Resource.Sigim.ErrorMessages.CampoObrigatorio, "Data da emissão")'
                },
                '@Html.NameFor(m => m.EntradaMaterial.DataEntregaNota)': {
                    required: '@string.Format(GIR.Sigim.Application.Resource.Sigim.ErrorMessages.CampoObrigatorio, "Data da entrega")',
                    ehDataEntregaMenorOuIgualDataEM: '@GIR.Sigim.Application.Resource.OrdemCompra.ErrorMessages.DataEntregaMaiorQueDataEntradaMaterial'
                },
                '@Html.NameFor(m => m.Quantidade)': {
                    decimalGreaterThanZero: "A Quantidade deve ser maior que zero."
                },
                '@Html.NameFor(m => m.Material.Id)': {
                    required: '@string.Format(GIR.Sigim.Application.Resource.Sigim.ErrorMessages.CampoObrigatorio, "Material")'
                },
                '@Html.NameFor(m => m.OrdemCompraId)': {
                    required: '@string.Format(GIR.Sigim.Application.Resource.Sigim.ErrorMessages.CampoObrigatorio, "OC")'
                },
                '@Html.NameFor(m => m.DataVencimentoLiberado)': {
                    required: '@string.Format(GIR.Sigim.Application.Resource.Sigim.ErrorMessages.CampoObrigatorio, "Data vencimento lib.")',
                    date: '@GIR.Sigim.Application.Resource.Sigim.ErrorMessages.InformeDataValida',
                    ehDataVencimentoNoLimitePagamento: "Data de vencimento fora do limite de pagamento"
                },
                '@Html.NameFor(m => m.ValorParcelaLiberado)': {
                    decimalGreaterThanZero: "O valor deve ser maior que zero.",
                    ehValorLiberadoMenorIgualPendente: "Valor inválido para ordem de compra",
                    ehValorLiberadoMenorIgualParcela: "Valor liberado maior que valor da parcela."
                },
                '@Html.NameFor(m => m.ImpostoFinanceiroId)': {
                    required: '@string.Format(GIR.Sigim.Application.Resource.Sigim.ErrorMessages.CampoObrigatorio, "Imposto")'
                },
                '@Html.NameFor(m => m.BaseCalculoImposto)': {
                    required: '@string.Format(GIR.Sigim.Application.Resource.Sigim.ErrorMessages.CampoObrigatorio, "Base de cálculo")'
                },
                '@Html.NameFor(m => m.DataVencimentoImposto)': {
                    date: '@GIR.Sigim.Application.Resource.Sigim.ErrorMessages.InformeDataValida'
                },
            },
            errorPlacement: function (error, element) {
                var span = getErrorMessageContainer(element.attr("name"));
                span.html(error.append());
            },
            highlight: function (element, errorClass) {
                var span = getErrorMessageContainer($(element).attr("name"));
                span.removeClass("field-validation-valid");
                span.addClass("field-validation-error");
            },
            unhighlight: function (element, errorClass) {
                var span = getErrorMessageContainer($(element).attr("name"));
                span.removeClass("field-validation-error");
                span.addClass("field-validation-valid");
            }
        });
    });

    function validaDataNota() {
        var isValid = true;
        if ($("#@Html.IdFor(m => m.EntradaMaterial.DataEmissaoNota)").val() != '') {
            isValid = false;
            $.ajax({
                type: 'POST',
                url: '@Url.Action("ValidaDataEmissaoNota")',
                cache: false,
                async: false,
                contentType: 'application/json; charset=utf-8',
                data: JSON.stringify({
                    '@Html.NameFor(m => m.EntradaMaterial.Data)': $("#@Html.IdFor(m => m.EntradaMaterial.Data)").val(),
                    '@Html.NameFor(m => m.EntradaMaterial.DataEmissaoNota)': $("#@Html.IdFor(m => m.EntradaMaterial.DataEmissaoNota)").val(),
                    '@Html.NameFor(m => m.EntradaMaterial.DataEntregaNota)': $("#@Html.IdFor(m => m.EntradaMaterial.DataEntregaNota)").val()
                })
            })
            .success(function (result) {
                showErrorMessage(getErrorMessageContainer('@Html.NameFor(m => m.EntradaMaterial.DataEmissaoNota)'), result.errorMessage);
                isValid = result.ehValido;
            });
        }

        return isValid;
    }

    function obterQuantidadeDiasMinimoParaPagamento() {
        var diasParaPagamento = 0;
        $.ajax({
            type: 'POST',
            url: '@Url.Action("ObterParametros", "Parametros")',
            cache: false,
            async: false,
            contentType: 'application/json; charset=utf-8'
        })
        .success(function (result) {
            diasParaPagamento = result.DiasPagamento;
        });

        return diasParaPagamento;
    }

    function liberarTitulos() {
        $.SmartMessageBox({
            title: "<i class='fa fa-question-circle txt-color-blue'></i> Confirma a liberação dos títulos para o Financeiro?",
            content: "Esta ação não poderá ser revertida.",
            buttons: '[Não][Sim]'
        }, function (ButtonPressed) {
            if (ButtonPressed === "Sim") {
                $.ajax({
                    type: 'POST',
                    url: '@Url.Action("LiberarTitulos", "EntradaMaterial")',
                    cache: false,
                    //async: false,
                    contentType: 'application/json; charset=utf-8',
                    data: JSON.stringify({
                        id: '@Model.EntradaMaterial.Id'
                    })
                })
                .success(function (result) {
                    $('#notificationMessages').html(result);
                })
            }
        });
    }

    function showMessages(messages) {
        $(messages).each(function () {
            var typeMessage = '';
            var titleMessage = '';
            switch (this.Type.toString()) {
                case '@((int)GIR.Sigim.Infrastructure.Crosscutting.Notification.TypeMessage.Info)':
                    typeMessage = 'info';
                    titleMessage = 'Informação';
                    break;
                case '@((int)GIR.Sigim.Infrastructure.Crosscutting.Notification.TypeMessage.Success)':
                    typeMessage = 'success';
                    titleMessage = 'Sucesso';
                    break;
                case '@((int)GIR.Sigim.Infrastructure.Crosscutting.Notification.TypeMessage.Warning)':
                    typeMessage = 'warning';
                    titleMessage = 'Alerta';
                    break;
                case '@((int)GIR.Sigim.Infrastructure.Crosscutting.Notification.TypeMessage.Error)':
                default:
                    typeMessage = 'error';
                    titleMessage = 'Erro';
                    break;
            }
            smartAlert(titleMessage, this.Text, typeMessage);
        });
    }

    function atualizarTotais() {
        var jsonItens = JSON.parse($("#@Html.IdFor(m => m.JsonItens)").val());
        var jsonImpostos = JSON.parse($("#@Html.IdFor(m => m.JsonImpostos)").val());
        var valorTotalItens = 0;
        var valorTotalIPI = 0;
        var valorTotalICMS = 0;
        var valorTotalICMSST = 0;
        var valorTotalDescontoOrdemCompra = 0;
        var percentualTotalAliquota = 0;
        var valorTotalImposto = 0;
        var valorTotalRetido = 0;
        var valorTotalDesconto = 0;
        var valorSubTotal = 0;
        var valorTotalAcrescimo = 0;
        var valorTotal = 0;

        $.each(jsonItens, function (i, item) {
            var valorIPI = item.ValorTotal * item.PercentualIPI / 100;
            valorTotalIPI += valorIPI;
            valorTotalItens += item.ValorTotal;// + valorIPI - (item.ValorTotal * item.PercentualDesconto / 100);

            var valorDescontoOC = item.ValorTotal * item.OrdemCompraItem.PercentualDescontoOrdemCompra / 100;
            valorTotalDescontoOrdemCompra += valorDescontoOC;

            var valorICMS = item.BaseICMS * item.PercentualICMS / 100;
            valorTotalICMS += valorICMS;

            var valorICMSST = item.BaseICMSST * item.PercentualICMSST / 100;
            valorTotalICMSST += valorICMSST;
        });

        valorSubTotal = valorTotalItens - valorTotalDescontoOrdemCompra;

        $.each(jsonImpostos, function (i, imposto) {
            percentualTotalAliquota += imposto.ImpostoFinanceiro.Aliquota;
            valorTotalImposto += imposto.Valor;
            if (imposto.ImpostoFinanceiro.EhRetido)
                valorTotalRetido += imposto.Valor;
        });

        var desconto = parseFloat($("#@Html.IdFor(m => m.EntradaMaterial.Desconto)").val().replace(".", "").replace(",", "."));
        var ehDescontoPercentual = $('#@Html.IdFor(m => m.EntradaMaterial.EhDescontoPercentual)').prop('checked');
        var valorDesconto = ehDescontoPercentual ? (valorSubTotal * desconto / 100) : desconto;

        var valorDedutivel = valorTotalICMS + valorTotalRetido;
        valorTotalDesconto = valorDesconto + valorDedutivel;

        var valorFreteIncluso = parseFloat($("#@Html.IdFor(m => m.EntradaMaterial.FreteIncluso)").val().replace(".", "").replace(",", "."));
        valorTotalAcrescimo = valorFreteIncluso;

        valorTotal = valorTotalItens - valorTotalDesconto - valorTotalDescontoOrdemCompra + valorTotalAcrescimo;

        $("#@Html.IdFor(m => m.ValorTotalItem)").val(roundDecimal(floatToString(valorTotalItens), 2));
        $("#@Html.IdFor(m => m.ValorTotalDescontoOrdemCompra)").val(roundDecimal(floatToString(valorTotalDescontoOrdemCompra), 2));
        $("#@Html.IdFor(m => m.ValorSubTotal)").val(roundDecimal(floatToString(valorSubTotal), 2));
        $("#@Html.IdFor(m => m.ValorTotalDesconto)").val(roundDecimal(floatToString(valorTotalDesconto), 2));
        $("#@Html.IdFor(m => m.ValorTotalAcrescimo)").val(roundDecimal(floatToString(valorTotalAcrescimo), 2));
        $("#@Html.IdFor(m => m.ValorTotal)").val(roundDecimal(floatToString(valorTotal), 2));
        $("#@Html.IdFor(m => m.BaseCalculoImposto)").val(roundDecimal(floatToString(valorTotalItens), 2));

        $("#@Html.IdFor(m => m.ValorTotalICMS)").val(roundDecimal(floatToString(valorTotalICMS), 2));
        $("#@Html.IdFor(m => m.ValorTotalIPI)").val(roundDecimal(floatToString(valorTotalIPI), 2));
        $("#@Html.IdFor(m => m.ValorTotalICMSST)").val(roundDecimal(floatToString(valorTotalICMSST), 2));

        $("#@Html.IdFor(m => m.PercentualTotalAliquota)").val(roundDecimal(floatToString(percentualTotalAliquota), 5));
        $("#@Html.IdFor(m => m.ValorTotalImposto)").val(roundDecimal(floatToString(valorTotalImposto), 2));
        $("#@Html.IdFor(m => m.ValorTotalRetido)").val(roundDecimal(floatToString(valorTotalRetido), 2));

        $("#@Html.IdFor(m => m.ValorTotalEMSelecaoFormaPagamento)").val(roundDecimal(floatToString(valorTotal + valorTotalDesconto), 2));
        $("#@Html.IdFor(m => m.ValorTotalEM)").val(roundDecimal(floatToString(valorTotal + valorTotalDesconto), 2));

        initializeDecimalBehaviour();
    }
</script>