@model IEnumerable<GIR.Sigim.Application.DTO.Sigim.TreeNodeDTO>

<ul>
    <li class="rootNode">
        <span><i class="fa fa-lg fa-minus-circle"></i> @ViewBag.FirstNode</span>
        <ul>
            @foreach (var item in Model)
            {
                @Html.Partial("~/Views/Shared/EditorTemplates/TreeNode.cshtml", item)
            }
        </ul>
    </li>
</ul>